<?xml version="1.0" ?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <kevin_payment>
                <active>0</active>
                <title>Kevin Payments</title>
                <order_status>pending</order_status>
                <payment_list>0</payment_list>
                <redirect_preferred>0</redirect_preferred>
                <order_email_after>1</order_email_after>
                <allowspecific>0</allowspecific>
                <can_use_checkout>1</can_use_checkout>
                <can_use_internal>0</can_use_internal>
                <can_initialize>0</can_initialize>
                <can_refund>1</can_refund>
                <can_refund_partial_per_invoice>0</can_refund_partial_per_invoice>
                <group>offline</group>
                <sort_order>1</sort_order>
                <client_id backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <client_secret backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <model>KevinPaymentFacade</model>
            </kevin_payment>
        </payment>
    </default>
</config>
